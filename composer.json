{
    "name": "sergant210/laravel-raw-view",
    "type": "library",
    "description": "It's intended to compile a view from the HTML string 'on the fly'. You don't need to create a view file.",
    "keywords": ["laravel", "rawview", "view", "blade"],
    "homepage": "https://github.com/sergant210/laravel-raw-view",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Sergey Shlokov",
            "email": "sergant210@bk.ru",
            "homepage": "https://modzone.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/sergant210/laravel-raw-view/issues"
    },
    "require": {
        "php" : "~7.0",
        "illuminate/view": "~5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Sergant210\\RawView\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    }
}
